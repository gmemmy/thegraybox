    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-check-file": "^3.3.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.3.3"
  },
  "packageManager": "bun@1.2.x",
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-screen-transitions",
          "eslint-config-expo",
